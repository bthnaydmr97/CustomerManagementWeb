@model CustomerManagementWeb.ServiceReference1.CustomerDto
@{
    ViewBag.Title = "OrderHistory";
}


<div class="container">
    <div class="row">
        <div class="col-md-10">
            <p><strong>İsim Soyisim:</strong> @Model.Name @Model.Surname</p>
            <p><strong>Ürün İsmi:</strong> @Model.ProductName</p>
            <p><strong>Fiyatı:</strong> @Model.OrderPrice</p>

        </div>

    </div>

    <div class="col-6 text-left">
        <a href="/Customer/Index">
            <button type="button" class="btn btn-warning"> Geri Dön</button>
        </a>
    </div>
</div>